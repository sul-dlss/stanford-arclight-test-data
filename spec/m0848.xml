  <archdesc level="collection">
    <did id="did-1.8.1">
      <head>Descriptive Summary</head>
      <unittitle label="Title">Howe, Fanny. Papers, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive">1981-1995</unitdate>
      <unitid label="Collection number" countrycode="US" repositorycode="CSt">M0848</unitid>
      <origination label="Creator">
        <persname source="lcnaf">Howe, Fanny.</persname>
      </origination>
      <physdesc label="Extent">
        <extent>2.5 linear ft.</extent>
      </physdesc>
      <repository label="Repository">
        <corpname source="lcnaf">Stanford University. Libraries. Dept. of Special Collections and University Archives.</corpname>
      </repository>
      <abstract label="Abstract">Manuscripts, typescripts, and correspondence, the last including letters from Samuel Beckett, William Corbett, Mark Mirsky, and Alice Walker (one of Walker's containing reference to Tillie Olsen).</abstract>
      <langmaterial label="Language">
        <language langcode="en">English.</language>
      </langmaterial>
    </did>
