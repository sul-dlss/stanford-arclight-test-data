      <unittitle label="Title">Grover, Sherwood. Papers, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive">ca. 1922, 1947-1966, and n.d.</unitdate>
      <unitid label="Collection number" countrycode="US" repositorycode="CSt">M0806</unitid>
