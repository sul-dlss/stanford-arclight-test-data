         <unitid label="Call Number:" countrycode="US" repositorycode="US-CSt">SC0074</unitid>
         <origination label="Creator:">
            <corpname rules="dacs" source="ingest">Stanford University. Summer Festival of the Arts.</corpname>
         </origination>
         <unittitle label="Title:">Stanford University, Summer Festival of the Arts, records</unittitle>
         <unitdate normal="1964/1969"
                   type="inclusive"
                   label="Dates:"
                   calendar="gregorian"
                   era="ce"
                   datechar="creation">1964-1969</unitdate>
         <physdesc label="Physical Description:">
