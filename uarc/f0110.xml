            <c02 id="aspace_ref18_vrn" level="file">
               <did>
                  <unittitle>Ray Reddy, D. Espar, and A. Eisenson, "Hear, Here"</unittitle>
                  <unitid>110.6</unitid>
                  <physdesc altrender="whole">
                     <extent altrender="materialtype spaceoccupied">1 film reel(s) (16mm)</extent>
                  </physdesc>
