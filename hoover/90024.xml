        <did>
            <unittitle>Inès Keller-Frick papers</unittitle>
            <unitdate normal="1905/1999" type="inclusive">1905-1999</unitdate>
            <unitid label="Collection Number" countrycode="US" repositorycode="CSt-H">90024</unitid>
            <repository>
