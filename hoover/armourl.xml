    <archdesc level="collection">
        <did>
            <unittitle>Lester Armour papers</unittitle>
            <unitdate normal="1941/1953" type="inclusive">1941-1953</unitdate>
            <unitid label="Collection Number" countrycode="US" repositorycode="CSt-H">71065</unitid>
            <repository>
