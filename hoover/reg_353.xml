        <bioghist id="ref7">
            <head>Biographical Note</head>
            <chronlist>
                <chronitem>
                    <date>1898 June 22</date>
                    <event>Born, Excelsior, Minnesota</event>
                </chronitem>
                <chronitem>
                    <date>1916</date>
                    <event>Graduate, Gallatin County High School, Montana</event>
                </chronitem>
                <chronitem>
                    <date>1916-1917</date>
                    <event>Student, University of Wisconsin</event>
                </chronitem>
                <chronitem>
                    <date>1923-1924</date>
                    <event>News editor, 
                        <title render="italic">Japan Times</title>, Tokyo, Japan</event>
                </chronitem>
                <chronitem>
                    <date>1925-1931</date>
                    <event>Bureau manager, United Press, Peking and Shanghai, China; Manila, Philippine Islands</event>
                </chronitem>
                <chronitem>
                    <date>1925-1931</date>
                    <event>News editor, 
                        <title render="italic">Peking Daily News</title>, Peking, China</event>
                </chronitem>
                <chronitem>
                    <date>1931-1941</date>
                    <event>Editor, 
                        <title render="italic">Shanghai Evening Post &amp; Mercury</title>, Shanghai, China</event>
                </chronitem>
                <chronitem>
                    <date>1932-1949</date>
                    <event>Correspondent, 
                        <title render="italic">Christian Science Monitor</title></event>
                </chronitem>
                <chronitem>
                    <date>1941</date>
                    <event>Author, 
                        <title render="italic">Chungking Today</title></event>
                </chronitem>
                <chronitem>
                    <date>1943-1945</date>
                    <event>Editor, American editor, 
                        <title render="italic">Shanghai Evening Post &amp; Mercury</title></event>
                </chronitem>
                <chronitem>
