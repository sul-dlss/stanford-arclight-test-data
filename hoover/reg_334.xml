        <scopecontent>
          <p>Correspondence arranged alphabetically by last name of correspondent.</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.3" level="series">
        <did>
          <unittitle>Subject File, <unitdate calendar="gregorian" era="ce" type="inclusive">1918-1940.</unitdate></unittitle>
          <container>Box No. 2-4.</container>
        </did>
