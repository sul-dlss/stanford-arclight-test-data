    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1857</date>
          <event>Born</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1876</date>
          <event>Graduated from the Moscow Military Cadet School</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1877-78</date>
          <event>Fought in the Turkish War</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1904</date>
          <event>Promoted to General</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1904-05</date>
          <event>Director of Operations of the II Army during the Russo-Japanese War</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1912</date>
          <event>Commander of the XII Army Corps</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1914</date>
          <event>Chief of Staff of the Southwestern Front in Galicia</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1915</date>
          <event>Commander-in-Chief of the Northwestern Front</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1915-17</date>
          <event>Chief of Staff for the Supreme Commander-in-Chief Nicholas II</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>Military adviser to the Russian Provisional Government Organized the Volunteer Army</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918</date>
          <event>Participated in the First Kuban Campaign (Icy March) Died, Ekaterinodar</event>
        </chronitem>
      </chronlist>
    </bioghist>
