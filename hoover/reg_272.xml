        <chronitem>
          <date calendar="gregorian" era="ce">1901</date>
          <event>M.D., University of Chicago</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918-1919</date>
          <event>Physician with Red Cross in France and Belgium</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1926-1929</date>
          <event>Professor of Obstetrics and Gynecology, University of Minnesota</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1929</date>
          <event>Professor of Obstetrics and Gynecology, University of Chicago</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1930</date>
