    <did id="did-1.7.1">
      <head>Descriptive Summary</head>
      <unittitle label="Title">Liviu Floda Papers, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive" normal="1935/1996">1935-1996</unitdate>
<unitid label="Collection number" repositorycode="CSt-H" countrycode="US">95023</unitid>
      <origination label="Creator">
        <persname>Floda, Liviu</persname>
      </origination>
      <physdesc label="Extent">
        <extent>83 manuscript boxes, 1 oversize box</extent>
        <extent>(35.5 linear feet)</extent>
      </physdesc>
      <repository label="Repository">
        <corpname>Hoover Institution Archives</corpname>
        <address>
          <addressline>Stanford, California 94305-6010</addressline>
        </address>
      </repository>
      <abstract label="Abstract">Scripts and sound recordings of Radio Free Europe broadcasts to Romania, and related
correspondence, memoranda, and printed matter, relating to Romanian politics, society and
culture, the status of civil liberties in Romania, and activities of Romanians abroad.</abstract>
      <langmaterial label="Language">
	<language langcode="rum">Romanian</language> and 
        <language langcode="eng">English.</language>
      </langmaterial>
    </did>
