        <did>
            <unittitle>Gregory G. Shinkarenko papers</unittitle>
            <unitdate normal="1910/1942" type="inclusive">1910-1942</unitdate>
            <unitid label="Collection Number" countrycode="US" repositorycode="CSt-H">95005</unitid>
