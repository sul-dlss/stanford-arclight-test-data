      <unittitle label="Title">Paul Findley Papers, </unittitle>
	<unitdate calendar="gregorian" era="ce" type="inclusive" normal="1974/1985">1974-1985</unitdate>
      <unitid label="Collection number" repositorycode="CSt-H" countrycode="US">88002</unitid>
