    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1921, March 7</date>
          <event>born, Coos Bay, Oregon</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1943</date>
          <event>B.S. with honors, University of Oregon</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1949</date>
          <event>LL.B., Yale University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1950</date>
          <event>admitted to California bar</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1955-56</date>
          <event>president, California Young Republicans</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1957-1962</date>
          <event>member California Assembly</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1963-64</date>
          <event>special counsel, California Constitution Revision Commission</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1971-72</date>
          <event>Deputy Assistant Secretary of Defense for Environmental Quality, Department of Defense, Washington, D.C.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1972</date>
          <event>recipient, Meritorious Civilian Service Award, Department of Defense</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1972-present</date>
          <event>member, and chairman Council on Environmental Quality, Executive Office of the President, Washington, D.C.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1973-present</date>
          <event>Senior U.S. environmental advisor for the Economic Commission for Europe</event>
        </chronitem>
      </chronlist>
    </bioghist>
