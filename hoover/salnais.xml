        <did>
            <unittitle>Voldemars Salnais papers</unittitle>
            <unitdate normal="1901/1973" type="inclusive">1901-1973</unitdate>
            <unitid label="Collection Number" countrycode="US" repositorycode="CSt-H">75069</unitid>
            <repository>
