    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1877, June 20</date>
          <event>Born, Buffalo, New York</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1899</date>
          <event>Bachelor Degree, Yale University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1900-1911</date>
          <eventgrp>
            <event>Chairman of the Executive Committee, Director, G.M. &amp; O. Railroads</event>
            <event>Director, Crown Zellerbach Corporation</event>
            <event>President, Goodyear Lumber Company</event>
          </eventgrp>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1911-1917</date>
          <event>President, Norwich Lumber Company</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>Captain, Field Artillery, U.S. Army</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918</date>
          <event>Colonel; Commander, 307th Field Artillery</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918</date>
          <event>Commander, 81st Field Artillery, France</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918</date>
          <event>General Inspector, Transportation Corps, A.E.F.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918, May 2</date>
          <event>President, Coal Commission for Central Europe</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1919, September 5</date>
          <event>Member, Inter-Allied Commission on Upper Silesia</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1919, October</date>
          <event>Discharged honorably from the two last posts</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1919</date>
          <event>Major General, 1st Division, New York National Guard</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1929-1960</date>
          <event>Trustee, President, Museum of Modern Art, New York</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1944-1945</date>
          <event>Deputy Commander, American Red Cross, Pacific Ocean Areas</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1947</date>
          <event>Personal Representative of Secretary of War, American Zone, Germany and Austria</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1952</date>
          <event>Master of Laws, Norwich University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1958</date>
          <event>Doctor of Fine Arts, Adelphi College</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1964, April 23</date>
          <event>Died, Old Westbury, New York</event>
        </chronitem>
      </chronlist>
    </bioghist>
    <dsc id="dsc-1.7.5" type="analyticover">
      <head>Series Description</head>
      <c01 id="c01-1.7.5.2" level="series">
        <did>
          <unittitle>COAL COMMISSION FILE, <unitdate calendar="gregorian" era="ce">1919.</unitdate></unittitle>
          <container type="Box">Boxes 1-2</container>
        </did>
        <odd>
          <p>Reports, resolutions, agreements, lists, protocols, newsclippings, press releases, press translations, maps and other documents produced by the Coal Commission, arranged by physical form</p>
        </odd>
      </c01>
      <c01 id="c01-1.7.5.3" level="series">
        <did>
          <unittitle>CORRESPONDENCE, <unitdate calendar="gregorian" era="ce">1920-1922.</unitdate></unittitle>
          <container type="Box" label="Box: ">2</container>
        </did>
        <scopecontent>
          <arrangement>
            <p>Arranged alphabetically by name of correspondent</p>
          </arrangement>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.5.4" level="series">
        <did>
          <unittitle>DIARY, <unitdate calendar="gregorian" era="ce">1919.</unitdate></unittitle>
          <container type="Box" label="Box: ">2</container>
        </did>
        <scopecontent>
          <p>Goodyear's daily activities between August 21, 1919 and September 23, 1919</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.5.5" level="series">
        <did>
          <unittitle>SPEECHES AND WRITINGS, <unitdate calendar="gregorian" era="ce">1919-1920.</unitdate></unittitle>
          <container type="Box" label="Box: ">2</container>
        </did>
        <scopecontent>
          <p>Memoirs on political, social and economic conditions in Central Europe, and speeches delivered after Goodyear's return to the United States</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.5.6" level="series">
        <did>
          <unittitle>MISCELLANY, <unitdate calendar="gregorian" era="ce">1919.</unitdate></unittitle>
          <container type="Box" label="Box: ">2</container>
        </did>
        <scopecontent>
          <p>Calling cards, invitations, railway tickets, ID card, report, leaflet, materials connected with Goodyear's stay in Central Europe</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.5.7" level="series">
        <did>
          <unittitle>INTER-ALLIED COMMISSION OFFICE FILE, <unitdate calendar="gregorian" era="ce">1919.</unitdate></unittitle>
          <container type="Box" label="Box: ">3</container>
        </did>
        <odd>
          <p>Reports, correspondence, memoranda, newsclippings, leaflets, arranged by physical form</p>
        </odd>
      </c01>
      <c01 id="c01-1.7.5.8" level="series">
        <did>
          <unittitle>PHOTOGRAPHS.</unittitle>
          <container type="file">Photo file</container>
        </did>
        <odd>
          <p>See photo card catalog</p>
        </odd>
      </c01>
    </dsc>
