    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1888, July 1</date>
          <event>Born, Liberty, North Carolina</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1914</date>
          <event>B.A., School of Geology, Oxford University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1914-1915</date>
          <event>Served with the Commission for Relief in Belgium</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917-1919</date>
          <event>Military service -Timber acquisition officer</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1919-1922</date>
          <event>Research officer in Region One of the United States Forest Service</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1922</date>
          <event>Master's Degree, Oxford University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1922-1927</date>
          <event>Made five agricultural expeditions into Northwestern China</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1933</date>
          <event>Associate Chief of the Soil Conservation Service of the United States Department of Agriculture</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1937</date>
          <event>Chief of Research</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1939-1947</date>
          <event>Assistant Chief of the Soil Conservation Service</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1944</date>
          <event>Author, Palestine, <title render="italic">Land of Promise</title></event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1974</date>
          <event>Died</event>
        </chronitem>
      </chronlist>
    </bioghist>
