    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1903, February 5</date>
          <event>Born, Henderson, Kentucky</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1925</date>
          <event>B.S., United States Naval Academy</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1933</date>
          <event>M.S., University of California, Berkeley</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1939-1941</date>
          <event>Commanding Officer, U.S.S. <emph render="italic">Goff</emph></event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1942</date>
          <event>Commanding Officer, U.S.S. <emph render="italic">Winslow</emph></event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1942-1943</date>
          <event>Commander, United States Navy, Flag Secretary, Staff Commander, Destroyers Atlantic Fleet</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1943-1944</date>
          <event>Commander, Destroyer Division Thirty-Six Atlantic Fleet, commanded destroyers during Normandy Invasion</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1944-1945</date>
          <event>Captain, United States Navy, Commander, Destroyer Squadron Forty-Eight Pacific Fleet, commanded destroyers during Philippine Invasion and Okinawa Invasion</event>
        </chronitem>
      </chronlist>
    </bioghist>
