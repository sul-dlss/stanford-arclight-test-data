    <did id="did-1.7.1">
      <head>Descriptive Summary</head>
      <unittitle label="Title">Ethiopian Subject Collection, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive" normal="1962/2001">1962-2001</unitdate>
<unitid label="Collection Number" countrycode="us" repositorycode="CSt-H" encodinganalog="099">XX781</unitid>
<physdesc label="Collection Size" encodinganalog="300$a">
	<extent>5 manuscript boxes, 1 phonorecord, 2 phonotape cassettes </extent>
	<extent>(2.3 linear feet)</extent>
</physdesc>

<repository label="Repository">
	<corpname>Hoover Institution Archives </corpname> <address> <addressline>Stanford, California 94305-6010</addressline> </address></repository>


      <abstract label="Abstract">Electronic bulletins, pamphlets, reports, press releases, communiqu&#xE9;s,
newsletters, conference papers, clippings, and other printed matter, relating to social,
political and economic conditions in Ethiopia, and to drought and famine, revolutionary
movements, and post-1974 military conflicts in Ethiopia.</abstract>
      <langmaterial label="Language">
        <language langcode="eng">English.</language>
      </langmaterial>
    </did>
