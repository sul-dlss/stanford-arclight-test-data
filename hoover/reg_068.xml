    <dsc id="dsc-1.7.4" type="analyticover">
      <head>Series Description</head>
      <c01 id="c01-1.7.4.2" level="series">
        <did>
          <unittitle>SUBJECT FILE, <unitdate calendar="gregorian" era="ce" type="inclusive">1962-1994</unitdate></unittitle>
          <container>Box 1</container>
        </did>
        <scopecontent>
          <p>Brochure, correspondence, flyer, newsletter, printed matter, profile, record of
proceedings, report, statment and survey relating to civil rights, foreign relations,
social, political and military conditions in Ethiopia</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.3" level="series">
        <did>
          <unittitle>DROUGHT AND FAMINE, <unitdate calendar="gregorian" era="ce" type="inclusive">1973-1994 </unitdate></unittitle>
          <container>Box 1</container>
        </did>
        <scopecontent>
          <p>Articles, brochure, clippings, flyers, newsletters and printed matter relating to famine
and relief activities in Ethiopia</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.4" level="series">
        <did>
          <unittitle>REVOLUTIONARY MOVEMENTS, <unitdate calendar="gregorian" era="ce" type="inclusive">1979-1994 </unitdate></unittitle>
          <container>Box 2</container>
        </did>
        <scopecontent>
          <p>Brochure, newsletter, news release and printed matter relating to revolutionary movements
in Ethiopia</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.5" level="series">
        <did>
          <unittitle>INTERNATIONAL SUPPORT GROUPS, <unitdate calendar="gregorian" era="ce" type="inclusive">1981-1994 </unitdate></unittitle>
          <container>Box 2</container>
        </did>
        <scopecontent>
          <p>Newsletter, printed matter and report relating to the role of the support groups in
response to the human rights violations and democratic movements in Ethiopia</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.6" level="series">
        <did>
          <unittitle>AUDIO-VISUAL FILE, <unitdate calendar="gregorian" era="ce" datechar="single">1985</unitdate></unittitle>
          <container>Record cabinet</container>
        </did>
        <scopecontent>
          <p>Phonorecord, "Tam Tam Pour L'Ethiopie"</p>
        </scopecontent>
      </c01>
    </dsc>
