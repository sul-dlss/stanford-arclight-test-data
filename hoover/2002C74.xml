        <did>
            <unittitle>Carl Frederick Eifler papers</unittitle>
            <unitdate normal="1934/2001" type="inclusive">1934-2001</unitdate>
            <unitid label="Collection Number" countrycode="US" repositorycode="CSt-H">2002C74</unitid>
            <repository>
