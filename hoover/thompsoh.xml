      <unittitle label="Title">Harold Keith Thompson Collection, 
		  </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive" normal="1932/1993">1932-1993</unitdate>
<unitid label="Collection number" repositorycode="CSt-H" countrycode="US">55006</unitid>
      <origination label="Collector"><persname>Thompson, Harold Keith,</persname> 1922-</origination>
      <physdesc label="Collection Size" encodinganalog="300$a">
		<extent>23 manuscript boxes</extent>
		<extent>9.6 linear feet)</extent>
	</physdesc>
      <repository label="Repository">
        <corpname>Hoover Institution Archives</corpname>
        <address>
          <addressline>Stanford, California 94305-6010</addressline>
        </address>
