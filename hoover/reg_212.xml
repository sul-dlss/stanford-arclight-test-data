    <dsc id="dsc-1.7.4" type="analyticover">
      <head>Series Description</head>
      <c01 id="c01-1.7.4.2" level="series">
        <did>
          <unittitle>Subject File, <unitdate calendar="gregorian" era="ce" type="inclusive">1950-1955</unitdate></unittitle>
          <container>Box 1.-3.</container>
        </did>
        <scopecontent>
          <p>Bulletins, clippings, communiques, government publications, memoranda, pamphlets, press
releases, printed matter, reports, seminar papers and statements relating to economic,
political and social conditions in Kenya</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.3" level="series">
        <did>
          <unittitle>Drought and Famine, <unitdate calendar="gregorian" era="ce" type="inclusive">1974-1977</unitdate></unittitle>
          <container>Box 3.</container>
        </did>
        <scopecontent>
          <p>Printed matter and speech relating to drought and famine in Kenya</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.4" level="series">
        <did>
          <unittitle>Revolutionary Movements, <unitdate calendar="gregorian" era="ce" datechar="single">1988</unitdate></unittitle>
          <container>Box 3.</container>
        </did>
        <scopecontent>
          <p>Press releases and statements relating to revolutionary movements in Kenya</p>
        </scopecontent>
      </c01>
    </dsc>
