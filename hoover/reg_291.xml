    <did id="did-1.7.1">
      <head>Descriptive Summary</head>
      <unittitle label="Title">Thomas T. C. Gregory Papers, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive" normal="1918/1932">1918-1932</unitdate>
<unitid label="Collection number" repositorycode="CSt-H" countrycode="US">72059 </unitid>
      <origination label="Creator">
        <persname>Gregory, Thomas T. C., 1878-1933</persname>
      </origination>
      <physdesc label="Collection Size" encodinganalog="300$a"><extent>3 manuscript boxes, 2 envelopes, 1 oversize box </extent><extent>(2.8 linear feet)</extent></physdesc>
      <repository label="Repository">
        <corpname>Hoover Institution Archives</corpname>
        <address>
          <addressline>Stanford, California 94305-6010</addressline>
        </address>
      </repository>
      <abstract label="Abstract">Correspondence, reports, memoranda, and printed matter, relating to the work of the
American Relief Administration in Central Europe, the fall of the 1919 Bela Kun communist
regime in Hungary, and the 1928 Presidential campaign of Herbert Hoover.</abstract>
      <langmaterial label="Language">
        <language langcode="eng">English.</language>
      </langmaterial>
    </did>
