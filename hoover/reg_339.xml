        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1930-1938</date>
          <event>Assistant Professor of History, Stanford University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1938-1942</date>
          <event>Associate Professor of History, Stanford University</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1942-1947</date>
