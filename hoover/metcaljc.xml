      <unittitle label="Title">John C. Metcalfe Papers, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive">1935-1949</unitdate>
<unitid label="Collection number" repositorycode="CSt-H" countrycode="US">72069</unitid>
