                    <date>1967-1968</date>
                    <event>Vice Chancellor for Academic Affairs, Long Island University</event>
                </chronitem>
                <chronitem>
                    <date>1968-1969</date>
                    <event>Chancellor, Long Island University</event>
                </chronitem>
            </chronlist>
        </bioghist>
