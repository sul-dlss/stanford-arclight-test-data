      <c01 id="c01-1.7.5.3" level="series">
        <did>
          <unittitle>Correspondence.</unittitle>
          <container type="Box">Box No. 3 -4.</container>
        </did>
        <scopecontent>
          <p>Correspondence, 1899-1970, arranged alphabetically by name of correspondent.</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.5.4" level="series">
        <did>
          <unittitle>Miscellany.</unittitle>
          <container type="Box">Box No. 4.</container>
        </did>
        <scopecontent>
          <p>Certificates, affidavits, identification cards, etc.</p>
        </scopecontent>
      </c01>
