    <dsc id="dsc-1.7.4" type="analyticover">
      <head>Series Description</head>
      <c01 id="c01-1.7.4.2" level="series">
        <did>
          <unittitle>SUBJECT FILE, <unitdate calendar="gregorian" era="ce" type="inclusive">1929-1995.</unitdate></unittitle>
          <container>Boxes 1-9.</container>
        </did>
        <scopecontent>
          <p>Booklets, brochures, bulletins, flyers, government documents, leaflets, pamphlets, press
briefings, printed matter, reports and serial issues relating to economic, cultural,
political and social conditions in Nigeria</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.3" level="series">
        <did>
          <unittitle>MISCELLANEOUS FILE, n.d.</unittitle>
          <container>Box 9.</container>
        </did>
        <scopecontent>
          <p>Miscellaneous items relating to Nigeria</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.4" level="series">
        <did>
          <unittitle>OVERSIZE MATERIALS, <unitdate calendar="gregorian" era="ce" type="inclusive">1929-1991. </unitdate></unittitle>
          <container>Box 10.</container>
        </did>
        <scopecontent>
          <p>Brochures, magazines and press comments relating to various aspects of life in Nigeria</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.5" level="series">
        <did>
          <unittitle>AUDIO-VISUAL FILE, <unitdate calendar="gregorian" era="ce" type="inclusive">1976-1986.</unitdate></unittitle>
          <container>Record cabinet; Cassette cabinet; Tape cabinet</container>
        </did>
        <scopecontent>
          <p>8 phonorecords, 2 phonotape cassettes and 1 videotape cassette cabinet</p>
        </scopecontent>
      </c01>
