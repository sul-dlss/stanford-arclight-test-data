    <did id="did-1.7.1">
      <head>Descriptive Summary</head>
      <unittitle label="Title">Namibian Subject Collection, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive" normal="1920/2000">1920-2000</unitdate>
<unitid label="Collection Number" countrycode="us" repositorycode="CSt-H" encodinganalog="099">XX786</unitid>
      <physdesc label="Collection Size">
		<extent>7 manuscript boxes, 1 oversize box, 2 phonorecords, 1 videotape cassette.</extent>
		<extent>(3.5 linear feet)</extent>
	</physdesc>
      <repository label="Repository">
        <corpname>Hoover Institution Archives</corpname>
        <address>
          <addressline>Stanford, California 94305-6010</addressline>
        </address>
      </repository>
      <abstract label="Abstract">Electronic bulletins, pamphlets, serial issues, flyers, reports, and other printed
matter, relating to revolutionary movements in Namibia, especially the South West African
Peoples Organisation, and to political, social and economic conditions in Namibia.</abstract>
      <langmaterial label="Language">
        <language langcode="eng">English</language> and 
	<language langcode="afr">Afrikaans.</language>
      </langmaterial>
    </did>
