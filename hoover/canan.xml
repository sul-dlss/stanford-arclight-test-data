                </chronitem>
                <chronitem>
                    <date>1919</date>
                    <event>A.E.F., France and Germany</event>
                </chronitem>
                <chronitem>
                    <date>1925-1928</date>
                    <event>11th Engineers, Panama Canal Zone</event>
                </chronitem>
                <chronitem>
                    <date>1932-1936</date>
                    <event>District Engineer, U.S. Lake Survey</event>
                </chronitem>
                <chronitem>
                    <date>1938-1941</date>
                    <event>War Department General Staff (G-2 Div.)</event>
                </chronitem>
                <chronitem>
                    <date>1943-1946</date>
                    <event>Office of the Chief Engineer, European Theater of Operations</event>
                </chronitem>
                <chronitem>
                    <date>1946-1949</date>
                    <event>District Engineer, Nashville, Tennessee</event>
                </chronitem>
                <chronitem>
                    <date>1952-1953</date>
                    <event>Engineer Inspector General</event>
                </chronitem>
                <chronitem>
                    <date>1953-1954</date>
