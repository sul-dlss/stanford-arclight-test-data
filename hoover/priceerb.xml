    <did id="did-1.7.1">
      <head>Descriptive Summary</head>
      <unittitle label="Title">Ernest Batson Price Papers, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive">1914-1960</unitdate>
<unitid label="Collection number" repositorycode="CSt-H" countrycode="US">74048</unitid>
      <origination label="Creator"><persname rules="aacr2">Price, Ernest Batson, </persname>1890-</origination>
      <physdesc label="Collection Size">
		<extent>13 manuscript boxes, 1 envelope</extent>
		<extent>(5.3 linear feet)</extent>
	</physdesc>
      <repository label="Repository">
        <corpname>Hoover Institution Archives</corpname>
        <address>
          <addressline>Stanford, California 94305-6010</addressline>
        </address>
      </repository>
      <abstract label="Abstract">Correspondence, diary, dispatches, writings, lecture material, photographs, and printed matter, relating to Japanese military intervention in China and Manchuria, 1931-1945; political and economic development in China, 1914-1950; and American foreign relations with China, 1914-1929.</abstract>
<physloc label="Physical Location">Hoover Institution Archives</physloc>
      <langmaterial label="Language">
        <language langcode="eng">English.</language>
      </langmaterial>
    </did>
