    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1872</date>
          <event>Born, Roubaix, Northern France</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1916</date>
          <event>Industrial advisor to Russia</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>Minister of Armament (France)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1919</date>
          <event>Participant in the peace negotiations at Versailles.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1921</date>
          <event>Minister of Liberated Territories</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1922</date>
          <event>Minister of Commerce</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1921-24</date>
          <event>Negotiator on reparation problems</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1927-28</date>
          <event>Minister of Labor, Hygiene and Social Security</event>
        </chronitem>
      </chronlist>
    </bioghist>
