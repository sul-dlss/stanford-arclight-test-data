          <event>Cavalry officer, Russian Army</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918-1921</date>
          <event>2nd Legion Infantry Division</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1921</date>
          <event>General Staff, 2nd Lancers Regiment</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1925</date>
          <event>General Staff Academy graduate</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1925-1935</date>
          <event>War Academy lecturer, Field service</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1935-1938</date>
          <event>Chief of Staff Army Inspectorate</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1938-1939</date>
          <event>Military attach&#xE9;, Kaunas, Lithuania</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1940-1941</date>
          <eventgrp>
