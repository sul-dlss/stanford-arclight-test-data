    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1864, Nov. 21</date>
          <event>Born, Vienna, Austria</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1892</date>
          <event>co-founded, with Bertha von Suttner, the the pacifist periodical <title render="italic">Die Waffen Nieder </title>(Lay Down Your Arms), from 1899 <title render="italic">Die Friedenswarte fuer Zwischenstaatliche Organization </title>(The Peace Watch for International Organization)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1894</date>
          <event>Author, <title render="italic">Friedenskatechismus </title>(Catechism of Peace)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1899</date>
          <event>Author, <title render="italic">Die Haager Konferenz, ihre Bedeutung, ihre Ergebnisse </title>(The Haag Conference, its Meaning, its Results)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1905</date>
          <event>Author, <title render="italic">Das Handbuch der Friedensbewegung </title>(Handbook of the Peace Movement)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1908</date>
          <event>Author, <title render="italic">Das Internationale Leben der Gegenwart </title>(International Living in a Modern World)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1910</date>
          <event>Author, <title render="italic">Pan-Amerika </title>(Pan-America)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1911</date>
          <event>Winner, Nobel Peace Prize</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1912</date>
          <event><title render="italic">Die Grundlagen des Ursaechlichen Pazifismus </title>(The Basics of Causative Pacifism)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1914</date>
          <event>Emigrated to Switzerland</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1914-1918</date>
          <event>Editor, <title render="italic">Blaetter fuer Internationale Verstaendigung und Zwischenstaatliche Organization </title>(Papers for International Understanding and Inter-State Organization)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918</date>
          <event>Author, <title render="italic">Der Weltprotest Gegen den Versailler Frieden </title>(The Global Protest Against the Versailles Treaty)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918-1920</date>
          <event>Author, <title render="italic">Mein Kriegstagebuch </title>(My War Diary), 4 volumes</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1921</date>
          <event>Died, Vienna, Austria</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1925</date>
          <event>Author, <title render="italic">Jugenderinnerungen </title>(Childhood Memoirs), published posthumously</event>
        </chronitem>
      </chronlist>
    </bioghist>
    <dsc id="dsc-1.7.5" type="analyticover">
      <head>Series Description</head>
      <c01 id="c01-1.7.5.2" level="series">
        <did>
          <unittitle>DIARIES, <unitdate calendar="gregorian" era="ce" type="inclusive">1914-1919. </unitdate></unittitle>
          <container type="Box">Boxes 1-2</container>
        </did>
        <odd>
