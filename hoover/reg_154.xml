    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <chronitem>
          <date calendar="gregorian" era="ce">1898, July 20</date>
          <event>Born, Minousinsk, Siberia</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>Graduated form the Alexander Military Academy, Moscow</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>Served in 95th Infantry Regiment</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918</date>
          <event>Served in the 2nd Kazan Infantry Regiment</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1918-1920</date>
          <event>Served in the Marine Corps under Admiral Kolchak</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1921-1923</date>
          <event>Served in the Siberian Flotilla</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1928</date>
          <event>Graduted from New York State College of Forestry, Syracuse University and entered the service of the Intercontinental Rubber Company</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1930</date>
          <event>Appointed as consulting forester for the Insular Lumber Company</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1931</date>
          <event>Entered the service of the United States Rubber Company</event>
        </chronitem>
      </chronlist>
    </bioghist>
