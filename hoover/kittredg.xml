        <bioghist id="ref7">
            <head>Biographical Note</head>
            <chronlist>
                <chronitem>
                    <date>1891</date>
                    <event>Born, Baker, Oregon</event>
                </chronitem>
                <chronitem>
                    <date>1914-17</date>
                    <event>Member and director, Educational Fund, Commission for Relief in Belgium</event>
                </chronitem>
                <chronitem>
                    <date>1917-19</date>
                    <event>Staff of Admiral William S. Sims, United States Naval Headquarters in Europe</event>
                </chronitem>
                <chronitem>
                    <date>1919</date>
                    <event>Staff, Supreme Economic Council, Paris Peace Conference</event>
                </chronitem>
                <chronitem>
                    <date>1920-31</date>
                    <event>Staff, League of Red Cross Societies</event>
                </chronitem>
                <chronitem>
                    <date>1921</date>
                    <event>Author, 
                        <title render="italic">Certain Naval Lessons of the Great War</title></event>
                </chronitem>
                <chronitem>
                    <date>1931-42</date>
                    <event>Assistant Director, Social Sciences Division, Rockefeller Foundation, European Office, Paris</event>
                </chronitem>
                <chronitem>
                    <date>1942-46</date>
                    <event>Staff of Admiral Harold R. Stark, American Naval Headquarters, London</event>
                </chronitem>
                <chronitem>
                    <date>1946-54</date>
                    <event>Senior Naval member, Historical Section, Joint Chiefs of Staff</event>
                </chronitem>
                <chronitem>
                    <date>1946</date>
                    <event>Author, 
                        <title render="italic">U.S.-British Naval Cooperation</title></event>
                </chronitem>
            </chronlist>
        </bioghist>
