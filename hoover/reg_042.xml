    <dsc id="dsc-1.7.4" type="analyticover">
      <head>Series Description</head>
      <c01 id="c01-1.7.4.2" level="series">
        <did>
          <unittitle>SUBJECT FILE, <unitdate calendar="gregorian" era="ce" type="inclusive">1935-1993. </unitdate></unittitle>
          <container>Boxes 1-2</container>
        </did>
        <scopecontent>
          <p>Reports, studies, memoranda, newletters, articles, pamphlets, printed matter and
miscellany. Arranged alphabetically by subject.</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.3" level="series">
        <did>
          <unittitle>SERIALS, <unitdate calendar="gregorian" era="ce" type="inclusive">1940-1993. </unitdate></unittitle>
          <container>Boxes 3-9</container>
        </did>
        <scopecontent>
          <p>Magazines and newspapers published in Estonia. Arranged alphabetically by title.</p>
        </scopecontent>
      </c01>
      <c01 id="c01-1.7.4.4" level="series">
        <did>
          <unittitle>PHONOTAPE CASSETTE, 1992.</unittitle>
          <container>Cassette cabinet</container>
        </did>
        <scopecontent>
          <p>One phonotape cassette recording of speeches of Estonian presidential candidates.</p>
        </scopecontent>
      </c01>
    </dsc>
