      <unittitle label="Title">Christian Joppke collection, </unittitle><unitdate calendar="gregorian" era="ce" type="inclusive" normal="1976/1995">1976-1995</unitdate>
      <unitid label="Collection number" repositorycode="CSt-H" countrycode="US">94027</unitid>
