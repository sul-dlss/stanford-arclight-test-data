    <bioghist id="bioghist-1.7.4">
      <head>Biographical Note</head>
      <chronlist>
        <head>Story, Russell McCulloch, 1883-1942</head>
        <chronitem>
          <date calendar="gregorian" era="ce">1883, April 9</date>
          <event>Born, Washburn, Illinois</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1904</date>
          <event>A.B. Monmouth (Ill.) College</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1908</date>
          <event>A.M., Harvard</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1909</date>
          <event>Married Gertrude Anderson of Pawnee City, Neb.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1909-10</date>
          <event>Teacher at Clark College</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1910-14</date>
          <event>Professor of History at Monmouth College</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1914-24</date>
          <event>Instructor, Asst. and Assoc. Professor, and Professor of Political Science, Univ. of Illinois.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>Ph.D., Harvard</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917</date>
          <event>author, THE AMERICAN MUNICIPAL EXECUTIVE</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1917-18</date>
          <event>War Work Secretary for Y.M.C.A. in Russia and Siberia</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1919</date>
          <event>decorated Cross of War (Czechoslovakia)</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1924-25</date>
          <event>Professor of Political Science, Syracuse U.</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1925-37</date>
          <event>Professor of Political Science, Pomona College</event>
        </chronitem>
        <chronitem>
          <date calendar="gregorian" era="ce">1937-42</date>
          <event>President of Claremont Colleges</event>
        </chronitem>
      </chronlist>
    </bioghist>
