        <persname source="lcnaf">Feierabend, Ladislav, 1891-1969</persname>
      </origination>
      <physdesc encodinganalog="300" label="Extent">
        <extent>21 manuscript boxes</extent>
        <extent>(8.8 linear feet)</extent>
      </physdesc>
      <repository label="Repository">
