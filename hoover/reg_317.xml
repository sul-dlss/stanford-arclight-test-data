        <bioghist id="ref8">
            <head>Biographical Note</head>
            <chronlist>
                <chronitem>
                    <date>1894 September</date>
                    <event>Born, North Carolina</event>
                </chronitem>
                <chronitem>
                    <date>1912</date>
                    <event>Joined United States Army as a private of the Cavalry branch</event>
                </chronitem>
                <chronitem>
                    <date>1915</date>
                    <event>Noncommissioned Officer, Coast Artillery Corps</event>
                </chronitem>
                <chronitem>
                    <date>1917</date>
                    <event>Second Lieutenant, Infantry branch</event>
                </chronitem>
                <chronitem>
                    <date>1917-1919</date>
                    <event>Served with American Expeditionary Force in France, attained rank of brevet Major</event>
                </chronitem>
                <chronitem>
                    <date>1931-1932</date>
                    <event>Served at Zamboanga, Mindanao, Philippine Islands</event>
                </chronitem>
                <chronitem>
                    <date>1932-1933</date>
                    <event>Served at Vancouver Barracks, Washington</event>
                </chronitem>
                <chronitem>
                    <date>1941</date>
                    <event>Colonel, United States Army</event>
                </chronitem>
                <chronitem>
                    <date>1942-1946</date>
                    <event>Regimental commander, 321st Infantry Regiment, 81st Division, (1942-1944, trained in continental United States; 1944-1945, fought in Pacific campaigns, 1945-1946, served with occupation troops on Kyushu, Japan).</event>
                </chronitem>
                <chronitem>
                    <date>1946</date>
                    <event>Resigned from United States Army.</event>
                </chronitem>
            </chronlist>
        </bioghist>
